    "@aws-sdk/client-dynamodb": "^3.282.0",
    "@aws-sdk/client-rekognition": "^3.282.0",
    "@aws-sdk/client-s3": "^3.282.0",
    "@aws-sdk/client-sns": "^3.282.0",
    "@aws-sdk/client-ssm": "^3.282.0",
    "@aws-sdk/client-sts": "^3.282.0",
